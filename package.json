{
  "name": "newtype",
  "private": false,
  "author": "Tom Fan",
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "javascript",
    "typescript",
    "utility",
    "development",
    "tools",
    "library",
    "typescript-library",
    "web-development",
    "nodejs"
  ],
  "description": "A TypeScript library packed with handy functions designed to accelerate development across different platforms.",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./array": {
      "import": "./dist/array.js",
      "require": "./dist/array.js"
    },
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.js"
    },
    "./date": {
      "import": "./dist/date.js",
      "require": "./dist/date.js"
    },
    "./math": {
      "import": "./dist/math.js",
      "require": "./dist/math.js"
    },
    "./regex": {
      "import": "./dist/regex.js",
      "require": "./dist/regex.js"
    },
    "./string": {
      "import": "./dist/string.js",
      "require": "./dist/string.js"
    },
    "./system": {
      "import": "./dist/system.js",
      "require": "./dist/system.js"
    }
  },
  "types": "./dist/index.d.ts",
  "version": "0.0.2",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.4.1",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
